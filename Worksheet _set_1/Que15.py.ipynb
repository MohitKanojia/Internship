   "id": "c95e3c91",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the stringMHJMKKNHY\n",
      "{'M': 1}\n",
      "{'M': 1, 'H': 1}\n",
      "{'M': 1, 'H': 1, 'J': 1}\n",
      "{'M': 2, 'H': 1, 'J': 1, 'K': 1}\n",
      "{'M': 2, 'H': 1, 'J': 1, 'K': 2, 'N': 1}\n",
      "{'M': 2, 'H': 2, 'J': 1, 'K': 2, 'N': 1, 'Y': 1}\n"
     ]
    }
   ],
   "source": [
    "str1 = input (\"Enter the string\")\n",
    "d1 = dict()\n",
    "for c in str1:\n",
    "    if c in d1:\n",
    "        d1[c] = d1[c] + 1\n",
    "    else:\n",
    "        d1[c] = 1\n",
    "        print(d1)"
   ]
  }
